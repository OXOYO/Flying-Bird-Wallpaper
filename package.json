{
  "name": "flying-bird-wallpaper",
  "productName": "Flying Bird Wallpaper",
  "version": "1.0.2",
  "description": "飞鸟壁纸是一款精美图片浏览器，让你轻松浏览、收藏心动美图，并一键设置壁纸，打造独一无二的桌面风格。",
  "type": "module",
  "main": "./out/main/index.js",
  "author": "OXOYO",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OXOYO/Flying-Bird-Wallpaper.git"
  },
  "bugs": {
    "url": "https://github.com/OXOYO/Flying-Bird-Wallpaper/issues"
  },
  "homepage": "https://github.com/OXOYO/Flying-Bird-Wallpaper#readme",
  "scripts": {
    "clean": "rimraf out dist",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "build:h5_watch": "vite build --config h5.vite.config.mjs --watch",
    "build:h5": "vite build --config h5.vite.config.mjs",
    "preview:app": "npm run clean && npm run build:h5 && electron-vite preview",
    "preview:app_win": "chcp 65001 && npm run clean && npm run build:h5 && electron-vite preview",
    "dev": "concurrently -k \"npm run build:h5_watch\" \"electron-vite dev\"",
    "dev:win": "chcp 65001 && concurrently -k \"npm run build:h5_watch\" \"electron-vite dev\"",
    "build:app": "electron-vite build",
    "build": "npm run clean && npm run build:h5 && npm run build:app",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "postinstall": "electron-builder install-app-deps",
    "version": "node scripts/set-version.js",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "postinstall": "electron-builder install-app-deps",
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@element-plus/icons-vue": "^2.3.1",
    "@iconify-json/ep": "^1.2.2",
    "@iconify-json/icon-park-solid": "^1.2.2",
    "@iconify-json/lucide": "^1.2.31",
    "@iconify-json/material-symbols": "^1.2.16",
    "@iconify-json/material-symbols-light": "^1.2.15",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/stash": "^1.2.2",
    "@iconify/utils": "^2.3.0",
    "@iconify/vue": "^4.3.0",
    "@koa/bodyparser": "^5.1.1",
    "@koa/router": "^13.1.0",
    "@node-rs/jieba": "^1.10.4",
    "@vant/icons": "^3.0.2",
    "@vue/server-renderer": "^3.5.13",
    "axios": "^1.7.9",
    "better-sqlite3": "^11.9.1",
    "cheerio": "^1.0.0",
    "clipboardy": "^4.0.0",
    "ejs": "^3.1.10",
    "electron-localshortcut": "^3.2.1",
    "electron-updater": "6.3.9",
    "element-plus": "^2.9.6",
    "i18next": "^23.16.8",
    "i18next-vue": "^5.2.0",
    "koa": "^2.15.3",
    "koa-connect": "^2.1.0",
    "koa-pino-logger": "^4.0.0",
    "koa-static": "^5.0.0",
    "koffi": "^2.10.1",
    "lru-cache": "^11.0.2",
    "node-forge": "^1.3.1",
    "normalize-path": "^3.0.0",
    "pinia": "^2.3.0",
    "pino": "^9.6.0",
    "pino-roll": "^3.0.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^10.0.0",
    "vant": "^4.9.15",
    "viewerjs": "^1.11.7",
    "vue-virtual-scroller": "2.0.0-beta.8",
    "wallpaper": "^7.2.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.8",
    "@babel/preset-env": "^7.26.9",
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron/rebuild": "^3.7.1",
    "@rushstack/eslint-patch": "^1.10.3",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^33.4.1",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "glob": "^11.0.0",
    "prebuild-install": "^7.1.2",
    "prettier": "^3.4.2",
    "prettier-plugin-ejs": "^1.0.3",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "1.79.0",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^5.4.11",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-live-reload": "^3.0.4",
    "vue": "^3.5.13"
  },
  "engines": {
    "node": ">=20.18.0"
  }
}
