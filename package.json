{
  "name": "flying-bird-wallpaper",
  "productName": "Flying Bird Wallpaper",
  "version": "1.2.1",
  "description": "飞鸟壁纸是一款功能丰富的桌面壁纸软件，支持图片、视频、律动、纯色等多种壁纸类型，让您的桌面焕发独特魅力。",
  "type": "module",
  "main": "./out/main/index.js",
  "author": "OXOYO",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OXOYO/Flying-Bird-Wallpaper.git"
  },
  "bugs": {
    "url": "https://github.com/OXOYO/Flying-Bird-Wallpaper/issues"
  },
  "homepage": "https://github.com/OXOYO/Flying-Bird-Wallpaper#readme",
  "scripts": {
    "clean": "rimraf out dist",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "build:h5_watch": "vite build --config h5.vite.config.mjs --watch",
    "build:h5": "vite build --config h5.vite.config.mjs",
    "preview:app": "npm run clean && npm run build:h5 && electron-vite preview",
    "preview:app_win": "chcp 65001 && npm run clean && npm run build:h5 && electron-vite preview",
    "dev": "concurrently -k \"npm run build:h5_watch\" \"electron-vite dev\"",
    "dev:win": "chcp 65001 && concurrently -k \"npm run build:h5_watch\" \"electron-vite dev\"",
    "build:app": "electron-vite build",
    "build": "npm run clean && npm run build:h5 && npm run build:app",
    "postinstall": "electron-builder install-app-deps",
    "version": "node scripts/set-version.js",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:first": "npx conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "@electron-toolkit/utils": "^4.0.0",
    "@node-rs/jieba": "^2.0.1",
    "better-sqlite3": "^12.2.0",
    "electron-localshortcut": "^3.2.1",
    "electron-updater": "^6.6.2",
    "koffi": "^2.14.0",
    "onnxruntime-node": "^1.22.0-rev",
    "pino": "^9.9.0",
    "pino-roll": "^3.1.0",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "wallpaper": "^7.2.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@electron-toolkit/eslint-config": "^2.1.0",
    "@iconify/json": "^2.2.376",
    "@iconify/vue": "^5.0.0",
    "@koa/router": "^14.0.0",
    "@rushstack/eslint-patch": "^1.12.0",
    "@vant/auto-import-resolver": "^1.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "axios": "^1.11.0",
    "clipboardy": "^4.0.0",
    "concurrently": "^9.2.0",
    "conventional-changelog-angular": "^8.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^35.7.5",
    "electron-builder": "^26.0.12",
    "electron-devtools-installer": "^4.0.0",
    "electron-vite": "^4.0.0",
    "element-plus": "^2.11.5",
    "eslint": "^9.33.0",
    "eslint-plugin-vue": "^10.4.0",
    "fast-glob": "^3.3.3",
    "i18next": "^25.4.2",
    "i18next-vue": "^5.3.0",
    "koa": "^3.0.1",
    "koa-compress": "^5.1.1",
    "koa-static": "^5.0.0",
    "leafer-ui": "^1.9.4",
    "lru-cache": "^11.1.0",
    "node-forge": "^1.3.1",
    "pinia": "^3.0.3",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.6.2",
    "qrcode": "^1.5.4",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.90.0",
    "socket.io-client": "^4.8.1",
    "three": "^0.179.1",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-icons": "^22.2.0",
    "unplugin-vue-components": "^29.0.0",
    "vant": "^4.9.21",
    "viewerjs": "^1.11.7",
    "vite": "^7.1.3",
    "vite-bundle-analyzer": "^1.2.1",
    "vite-plugin-compression2": "^2.2.0",
    "vite-plugin-live-reload": "^3.0.5",
    "vue": "^3.5.19",
    "vue-eslint-parser": "^10.2.0"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}